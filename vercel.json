{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist/s8-inprocode/browser"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*\\.(js|css|png|jpg|ico|svg|json|woff2?|ttf))",
        "headers": { "Cache-Control": "public, max-age=31536000, immutable" },
        "continue": true
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }